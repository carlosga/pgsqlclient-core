{
    "title": "postgresql data provider for .net core",
    "version": "0.0.1",
    "compilationOptions": {
        "languageVersion" : "csharp6",
        "platform"        : "x64"
    },
    "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-*",
        "System.Collections": "4.0.11-rc2-*",
        "System.Collections.Concurrent": "4.0.12-rc2-*",
        "System.Data.Common": "4.0.1-rc2-*",
        "System.Globalization": "4.0.11-rc2-*",
        "System.IO": "4.1.0-rc2-*",
        "System.Linq": "4.1.0-rc2-*",
        "System.Net.NameResolution": "4.0.0-rc2-*",
        "System.Net.NetworkInformation": "4.1.0-rc2-*",
        "System.Net.Security": "4.0.0-rc2-*",
        "System.Net.Sockets": "4.1.0-rc2-*",
        "System.Security.Cryptography.Algorithms": "4.1.0-rc2-*",
        "System.Security.Cryptography.X509Certificates": "4.1.0-rc2-*",
        "System.Text.Encoding": "4.0.11-rc2-*",
        "System.Threading": "4.0.11-rc2-*",
        "System.Threading.Tasks": "4.0.11-rc2-*",
        "System.Threading.Thread": "4.0.0-rc2-*",
        "System.Threading.ThreadPool": "4.0.10-rc2-*"
    },
    "frameworks": {
        "netstandard1.5": { }
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "RELEASE",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "compile": "src/**/*.cs",
    "exclude": ["src/PostgreSql/Data/Connection Pooling/SqlClient/**"],
    "resource": "src/**/*.xml"
}
