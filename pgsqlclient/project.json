{
    "name": "pgsqlclient",
    "title": "postgresql ado.net provider for .net core",
    "version": "0.0.1",
    "compilationOptions": {
        "emitEntryPoint": false,
        "allowUnsafe": false,
        "warningsAsErrors" : false          
    },
    "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-23911",
        "Microsoft.Win32.Primitives": "4.0.1-rc2-23911",
        "System.Collections": "4.0.11-rc3-23910",
        "System.Collections.Concurrent": "4.0.12-rc2-23911",
        "System.Collections.NonGeneric": "4.0.1-beta-23516",
        "System.Data.Common": "4.0.1-rc3-23910",
        "System.Diagnostics.Debug": "4.0.11-rc3-23910",
        "System.Globalization": "4.0.11-rc3-23910",
        "System.IO": "4.1.0-rc2-23911",
        "System.Linq": "4.1.0-rc2-23911",
        "System.Net.NameResolution": "4.0.0-rc3-23910",
        "System.Net.Primitives": "4.0.11-rc3-23910",
        "System.Net.Security": "4.0.0-beta-23516",
        "System.Net.Sockets": "4.1.0-beta-23516",
        "System.Reflection": "4.1.0-rc3-23910",
        "System.Reflection.TypeExtensions": "4.1.0-beta-23516",
        "System.Resources.ResourceManager": "4.0.1-rc3-23910",
        "System.Runtime": "4.1.0-rc2-23911",
        "System.Runtime.Extensions": "4.1.0-rc2-23911",
        "System.Security.Cryptography.X509Certificates": "4.1.0-rc3-23910",
        "System.Text.Encoding": "4.0.11-rc3-23910",
        "System.Text.Encoding.CodePages": "4.0.0",
        "System.Text.Encoding.Extensions": "4.0.11-rc2-23911",
        "System.Text.RegularExpressions": "4.0.12-rc2-23911",
        "System.Threading": "4.0.11-rc3-23910",
        "System.Threading.Tasks": "4.0.11-rc3-23910",
        "System.Threading.Thread": "4.0.0-rc3-23910"
    },
    "frameworks": {
        "netstandard1.5": {
            "imports": "dnxcore50"
        }
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "RELEASE",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "compile": "PostgreSql/**/*.cs",
    "resource": "PostgreSql/**/*.xml",
    "exclude": ["PostgreSql/Data/Schema/**/*"]    
}