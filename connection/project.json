{
    "version": "0.0.1",
    "compilationOptions": {
        "emitEntryPoint": true,
        "allowUnsafe": false,
        "warningsAsErrors" : false       
    },
    "dependencies": {
	"NETStandard.Library": "1.5.0-rc2-23911",
        "pgsqlclient": {
            "target"  : "project"            
        }
    },
    "frameworks": {
        "netstandardapp1.5": {
            "imports": "dnxcore50"
        }
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "RELEASE",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "compile": "**/*.cs"
}
