{
    "version": "0.0.1",
    "compilationOptions": {
        "emitEntryPoint": true
    },
    "dependencies": {
        "NETStandard.Library": "1.5.0-rc3-*",
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0-rc2-3002363"
        },
        "System.Net.NetworkInformation": "4.1.0-rc3-*",
        "PostgreSql.Data.SqlClient": {
            "target"  : "project",
            "version" : "0.0.1"
        }
    },
    "frameworks": {
        "netcoreapp1.0": { }
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "RELEASE",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "compile": "src/**/*.cs"
}
