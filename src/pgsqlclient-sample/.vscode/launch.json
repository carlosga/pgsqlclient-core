{
    "version": "0.0.1",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "justMyCode": false,
            "program": "${workspaceRoot}/bin/Debug/netcoreapp1.0/pgsqlclient-sample.dll",
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processName": "<example>"
        }
    ]
}
