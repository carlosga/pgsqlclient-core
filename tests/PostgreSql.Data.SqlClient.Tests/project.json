{
    "title": "postgresql data provider for .net core. unit tests.",
    "version": "0.1.0",
    "buildOptions": {
        "languageVersion": "csharp6",
        "platform"       : "x64",
        "compile"        : "src/**/*.cs",
        "copyToOutput"   : ["xunit.runner.json", "ConnectionString.xml", "MultipleResultsTest.bsl"]
    },
    "dependencies": {
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0-rc2-*"
        },
        "Microsoft.CSharp": "4.0.1-rc2-*",
        "Microsoft.Extensions.DependencyModel": "1.0.0-*",
        "System.Collections": "4.0.11-rc2-*",
        "System.Collections.Concurrent": "4.0.12-rc2-*",
        "System.Collections.NonGeneric": "4.0.1-rc2-*",
        "System.Data.Common": "4.0.1-rc2-*",
        "System.Dynamic.Runtime": "4.0.11-rc2-*",
        "System.Globalization": "4.0.11-rc2-*",
        "System.IO": "4.1.0-rc2-*",
        "System.Linq": "4.1.0-rc2-*",
        "System.Reflection": "4.1.0-rc2-*",
        "System.Reflection.TypeExtensions": "4.1.0-rc2-*",
        "System.Resources.ResourceManager": "4.0.1-rc2-*",
        "System.Runtime": "4.1.0-rc2-*",
        "System.Runtime.Extensions": "4.1.0-rc2-*",
        "System.Runtime.Serialization.Primitives": "4.1.1-rc2-*",
        "System.Security.Cryptography.X509Certificates": "4.1.0-rc2-*",
        "System.Security.Cryptography.Algorithms": "4.1.0-rc2-*",
        "System.Text.Encoding": "4.0.11-rc2-*",
        "System.Threading": "4.0.11-rc2-*",
        "System.Threading.Tasks": "4.0.11-rc2-*",
        "System.Threading.Thread": "4.0.0-rc2-*",
        "System.Threading.ThreadPool": "4.0.10-rc2-*",
        "System.Xml.XmlDocument": "4.0.1-rc2-*",
        "xunit": "2.2.0-*",
        "dotnet-test-xunit": "1.0.0-rc2-*",
        "PostgreSql.Data.SqlClient": {
            "target"  : "project"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": [
                "dnxcore50",
                "portable-net45+win8"
            ]
        }
    },
    "testRunner":  "xunit"
}
