{
    "version": "0.0.1",
    "compilationOptions": {
        "emitEntryPoint": false,
        "allowUnsafe": false,
        "warningsAsErrors" : false       
    },
    "dependencies": {
        "NETStandard.Library": "1.0.0-rc2-23901",
        "xunit": "2.1.0",
        "dotnet-test-xunit": "1.0.0-dev-91790-12",
        "pgsqlclient": {
            "target"  : "project"            
        }
    },
    "frameworks": {
        "netstandard1.5": {
            "imports": "dnxcore50"
        }
    },
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "RELEASE",
                    "TRACE"
                ],
                "optimize": true
            }
        }
    },
    "compile": "**/*.cs",
    "testRunner": "xunit"
}